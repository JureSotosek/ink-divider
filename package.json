    "eslint-plugin-react": "^7.1.0",
    "ink": "^0.5.0",
    "semantic-release": "^15.9.1",
    "travis-deploy-once": "^5.0.1"
  },
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h",
          "useBuiltIns": true
        }
      ]
